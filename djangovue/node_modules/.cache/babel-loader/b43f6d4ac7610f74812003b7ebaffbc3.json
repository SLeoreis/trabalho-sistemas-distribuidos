>{{Geral.linguagem}}</td>\n            <td>{{Geral.palavras_chave}}</td>\n            <td>{{Geral.descricao}}</td>\n            <td>\n              <button @click=\"getOne(Geral)\" class=\"btn btn-warning\">Editar</button>\n            </td>\n                <td>\n              <button @click=\"deleteOne(Geral.id)\" class=\"btn btn-danger\">Excluir</button>\n            </td>\n          </tr> \n        </tbody>\n        </table>\n      </div>\n</div>\n</template>\n\n<script>\nimport axios from 'axios'\nexport default {\n  name: 'Container',\n  props: {\n    msg: String\n  },\n  data(){\n      return{\n        Geral:null,\n        id:'',\n        titulo:'',\n        linguagem:'',\n        descricao:'',\n        palavras_chave:'',\n        cobertura:'',\n        estrutura:'',\n        nivel_de_agregacao:'',\n      }\n  },\n  mounted(){\n    this.getAll();\n  },\n  methods:{\n    getAll(){\n      axios.get('http://127.0.0.1:8000/api/v1/geral')\n      .then((res)=>{\n        this.Geral=res.data;\n        this.id='';\n        this.titulo='';\n        this.linguagem='';\n        this.descricao='';\n        this.palavras_chave='';\n        this.cobertura='';\n        this.estrutura='';\n        this.nivel_de_agregacao='';\n      })\n    },\n    getOne(Geral){\n        this.id=Geral.id;\n        this.titulo= Geral.titulo;\n        this.linguagem= Geral.linguagem;\n        this.descricao= Geral.descricao;\n        this.palavras_chave= Geral.palavras_chave;\n        this.cobertura= Geral.cobertura;\n        this.estrutura= Geral.estrutura;\n        this.nivel_de_agregacao= Geral.nivel_de_agregacao;\n    },\n    deleteOne(id){\n      axios.delete(id,{auth:{\n        username:'postgres',\n        password:'1234'\n      }})\n      .then(()=>{\n            this.getAll();\n        })\n    },\n    postgeral(){\n      if(this.id==''){\n        axios.post('http://127.0.0.1:8000/api/v1/geral',\n        {id:this.id,titulo:this.titulo,linguagem:this.linguagem,descricao:this.descricao,\n        palavras_chave:this.palavras_chave,cobertura:this.cobertura, estrutura:this.estrutura,nivel_de_agregacao:this.nivel_de_agregacao},\n        {auth:{username:'postgres',password:'1234'}}\n        .then(()=>{\n            this.getAll();\n        })\n      )\n      }else{\n        axios.put(\n        {id:this.id,titulo:this.titulo,linguagem:this.linguagem,descricao:this.descricao,\n        palavras_chave:this.palavras_chave,cobertura:this.cobertura, estrutura:this.estrutura,nivel_de_agregacao:this.nivel_de_agregacao},\n        {auth:{username:'postgres',password:'1234'}}\n        .then(()=>{\n            this.getAll();\n        })\n      )\n      }\n        \n    }\n  }  \n}\n</script>\n"],"sourceRoot":"src/components"}]}